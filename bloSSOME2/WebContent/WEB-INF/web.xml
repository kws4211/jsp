    <servlet-name>AdminControl</servlet-name>
    <url-pattern>/AdminControl</url-pattern>
    <url-pattern>*.admin</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AppealControl</servlet-name>
    <servlet-class>blossome.control.AppealControl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AppealControl</servlet-name>
    <url-pattern>/AppealControl</url-pattern>
    <url-pattern>*.appeal</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FindControl</servlet-name>
    <servlet-class>blossome.control.FindControl</servlet-class>

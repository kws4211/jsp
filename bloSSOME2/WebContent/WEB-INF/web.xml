    <servlet-name>MemberControl</servlet-name>
    <url-pattern>/MemberControl</url-pattern>
    <url-pattern>*.member</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>TukControl</servlet-name>
    <servlet-class>blossome.control.TukControl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TukControl</servlet-name>
    <url-pattern>/TukControl</url-pattern>
    <url-pattern>*.tuk</url-pattern>
  </servlet-mapping>

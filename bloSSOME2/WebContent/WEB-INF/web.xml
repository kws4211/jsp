    <servlet-name>LoginControl</servlet-name>
    <url-pattern>/LoginControl</url-pattern>
    <url-pattern>*.login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>MemberControl</servlet-name>
